section.landing
  .container
    .landing__wrap
      .landing__img.wow.fadeInLeft
        img(src=content.landing.img.src, alt=content.landing.img.alt)
      .landing__content.wow.fadeInRight
        .landing__info
          h2.landing__title= content.landing.title
          each val in content.landing.texts
            p.landing__text= val["item"]
          a.btn.landing__link(href=content.landing.link.href target="_blank")= content.landing.link.text