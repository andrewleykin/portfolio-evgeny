section.kitchen
  .container
    .kitchen__wrap
      .kitchen__info.wow.fadeInLeft
        .kitchen__header
          .kitchen__icon
            img(src=content.kitchen.icon.src, alt=content.kitchen.icon.alt)
          h2.kitchen__title= content.kitchen.title
        .kitchen__content
          p.kitchen__text= content.kitchen.text
          p.kitchen__text At the <a href="https://medium.com/" class="underline" target="_blank">Medium.com</a> portal you can see all details and watch how can i create so genious  project and see my creative process.
          a.btn.kitchen__link(href=content.kitchen.link.href target="_blank")= content.kitchen.link.text
      .kitchen__img.wow.fadeInRight